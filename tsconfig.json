{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
      "target": "ES2020",
      "alwaysStrict": true,
      "module": "ES2020",
      "experimentalDecorators": true,
      "noImplicitAny": false,
      "baseUrl": "./",
      "moduleResolution": "Node",
      "strict": true,
      "useDefineForClassFields": false, // breaks eosio-core
      "paths": {
          "crypto": [
              "./node_modules/crypto-js"
          ],
          "src/*": [
              "./src/*"
          ],
          "app/*": [
              "./*"
          ],
          "components/*": [
              "./src/components/*"
          ],
          "layouts/*": [
              "./src/layouts/*"
          ],
          "pages/*": [
              "./src/pages/*"
          ],
          "assets/*": [
              "./src/assets/*"
          ],
          "boot/*": [
              "./src/boot/*"
          ],
          "stores/*": [
              "./src/stores/*"
          ]
      },
      "types": [
          "pinia-plugin-persist",
          "vite/client",
          "node"
      ]
  },"buildOptions": {

  }
}
